<script>
(function() {
  // Add sale banner class to body
  document.body.classList.add('bn-sale-active');

  // Remove default-hidden class from all bn-sale-banner elements
  document.querySelectorAll('.bn-sale-banner').forEach(function(el) {
    el.classList.remove('default-hidden');
  });

  function updateTimer() {
    const timerElements = document.querySelectorAll('.bn-sale-banner__timer-value');

    timerElements.forEach(function(timer) {
      const saleEndDate = timer.getAttribute('data-sale-end');
      if (!saleEndDate) return;

      const endTime = new Date(saleEndDate).getTime();
      const now = new Date();

      // Calculate time remaining until sale end
      let totalDistance = endTime - now.getTime();

      // If sale ended, hide timer
      if (totalDistance < 0) {
        timer.closest('.bn-sale-banner__timer').style.display = 'none';
        return;
      }

      // Show countdown for current 24-hour window
      let distance = totalDistance % (24 * 60 * 60 * 1000);

      // If less than 24 hours remain, show actual time left
      if (totalDistance < 24 * 60 * 60 * 1000) {
        distance = totalDistance;
      }

      if (distance < 0) {
        timer.closest('.bn-sale-banner__timer').style.display = 'none';
        return;
      }

      const hours = Math.floor(distance / (1000 * 60 * 60));
      const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
      const seconds = Math.floor((distance % (1000 * 60)) / 1000);

      timer.textContent = hours + 'h:' +
                          (minutes < 10 ? '0' : '') + minutes + 'm:' +
                          (seconds < 10 ? '0' : '') + seconds + 's';
    });
  }

  // Update immediately and then every second
  updateTimer();

  // Show all timers
  document.querySelectorAll('.bn-sale-banner__timer').forEach(function(timer) {
    timer.style.removeProperty('display');
  });

  setInterval(updateTimer, 1000);
})();
</script>
